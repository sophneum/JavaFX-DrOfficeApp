<VBox prefWidth="1000" spacing="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yohealth.controllers.PatientController">
    <children>
        <Label fx:id="date" />
        <Label fx:id="messageName" />
        <Label fx:id="subject" />
        <Label  fx:id="content" wrapText="true" />
        <Button fx:id="backButtonMessageStyle" onAction="#onbackButtonMessageStyle" mnemonicParsing="false" text="Back" prefWidth="100.0" style="-fx-background-color: #71816D;" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
